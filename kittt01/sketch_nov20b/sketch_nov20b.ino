


void setup() {
  // initialize digital pin LED_BUILTIN as an output.
 
pinMode(1, OUTPUT);
pinMode(2, OUTPUT);
pinMode(3, OUTPUT);
pinMode(4, OUTPUT);

}

// the loop function runs over and over again forever
void loop() {
  digitalWrite(1, HIGH);  
  digitalWrite(2, HIGH);
  digitalWrite(3, HIGH);
  digitalWrite(4, HIGH);
 
}